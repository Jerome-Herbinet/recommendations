<?xml version="1.0"?>
<info xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	  xsi:noNamespaceSchemaLocation="https://apps.nextcloud.com/schema/apps/info.xsd">
	<id>recommendations</id>
	<name>Recommendations</name>
	<summary>Shows recommended files</summary>
	<description>Shows recommended files for quick access of files and folders with recent activity
	</description>
	<version>0.5.0</version>
	<licence>agpl</licence>
	<author>Christoph Wurst</author>
	<author>Jan-Christoph Borchardt</author>
	<namespace>Recommendations</namespace>
	<default_enable/>
	<category>office</category>
	<dependencies>
		<php min-version="7.1" max-version="7.3"/>
		<nextcloud min-version="17" max-version="17"/>
	</dependencies>
	<commands>
		<command>OCA\Recommendations\Command\GetRecommendations</command>
	</commands>
	<settings>
		<personal>OCA\Recommendations\Settings\Personal</personal>
		<personal-section>OCA\Recommendations\Settings\PersonalSection</personal-section>
	</settings>
</info>
